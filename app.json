{
  "expo": {
    "name": " Forex calc",
    "slug": "newForexCalc",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aleksbogdanov.my-forex-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.aleksbogdanov.my_forex_app",
      "versionCode": 1,
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    }
  }
}
